{
  "expo": {
    "name": "Rapfo",
    "slug": "Rapfo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/config/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/config/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#41AFEF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nnadozi.Rapfo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/config/adaptive-icon.png",
        "backgroundColor": "#41AFEF"
      },
      "package": "com.nnadozi.Rapfo"
    },
    "web": {
      "favicon": "./assets/images/config/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Figtree-Regular.ttf",
            "./assets/fonts/Figtree-Bold.ttf"
          ]
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "96e7efe4-c9c4-4ed6-9aca-aac5747cbc30"
      }
    }
  }
}
